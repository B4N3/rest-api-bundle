{
    "name": "troytft/reset-api-bundle",
    "type": "library",
    "description": "Symfony bundle for REST API",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "RestApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "doctrine/orm": "~2.5",
        "symfony/framework-bundle": "~3.4",
        "symfony/validator": "~3.3",
        "troytft/mapper": "~1.0.1"
    }
}
